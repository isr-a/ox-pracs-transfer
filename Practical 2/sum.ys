.pos 0
    irmovq stack, %rsp
    call main
    halt

.align 8
array:
    .quad 0x0000000000000001
    .quad 0x0000000000000002
    .quad 0x0000000000000003
    .quad 0x0000000000000004

main:
    irmovq array, %rdi
    irmovq $4, %rsi
    call sum
    ret
    
sum:
    irmovq $8, %r8
    irmovq $1, %r9
    xorq %rax, %rax
    andq %rsi, %rsi
    jmp test
loop:
    mrmovq (%rdi), %r10
    addq %r10, %rax
    addq %r8, %rdi
    subq %r9, %rsi
test:
    jne loop
    ret

.pos 0x200
stack:
